#include "util.h"
class NvaClase {
    int x;
public:
    NvaClase(int x): x(x){}
    ~NvaClase() { 
        cout << "Ejecutando el Metodo Destructor " << x << endl;
    }
};
NvaClase obj_Clase(1);
