import random as r

#GENERAR DATOS
codigo = r.randint(1000, 9999)
hora = r.randint(1,24)
minuto = r.randint(1,60)
segundo = r.randint(1,60)

print(f"El codigo generado es: {codigo}{hora}{minuto}{segundo}")
print(f"El codigo es: {codigo}")
print(f"La hora es: {hora}")
print(f"El minuto es: {minuto}")
print(f"El segundo es: {segundo}")
