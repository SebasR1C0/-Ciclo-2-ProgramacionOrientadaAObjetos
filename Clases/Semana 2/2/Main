
#include "iostream"
#include <conio.h>
#include <vector>
using namespace std;
int main() {
	vector<int> vector1(5);
	cout << "Numero de elementos del vector1: " << vector1.size() << endl;
	vector<int> vector2{ 5 };
	cout << "Numero de elementos del vector1: " << vector2.size() << endl;
	vector<int> vector3(5, 1);
	cout << "Listado del vector3 " << endl;
	for (int i = 0; i < vector3.size(); i++) {
		cout << vector3[i] << endl;
	}
	vector<int> vector4{ 10,15,20 };
	cout << "Listado del vector4 " << endl;
	for (int i = 0; i < vector4.size(); i++) {
		cout << vector4[i] << endl;
	}
	vector<int> vector5(vector4);
	for (int i = 0; i < vector5.size(); i++) {
		cout << "Vector 5: " << vector5[i] << endl;
	}
	getch();
	return 0;
}
